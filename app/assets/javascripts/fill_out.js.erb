PT = PT || {};

PT.aggregatUrl = "<%= ENV['AGGREGATOR_URL'] %>";

PT.retrieveSurveyDefinition = function(surveyCode, callBack){
  var url = PT.aggregatUrl + "/surveys/" + surveyCode;  
  console.log(url)
  $.get(url, function(data){
    PT.surveyDefinition = data.payload
    callBack(data.payload);
  });
};